{
	"_version": "1.7.0",
	"sap.app": {
		"id": "incentergy.bccrm.BusinessCardCRM",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "incentergy.bccrm.BusinessCardCRM.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "incentergy.bccrm.BusinessCardCRM.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "incentergy.bccrm.BusinessCardCRM.view",
				"controlId": "splitApp",
				"controlAggregation": "detailPages",
				"clearTarget": false,
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "Home",
					"target": [
						"homeDetails",
						"businessCardList"
					]
				},
				{
					"pattern": "homeDetails",
					"name": "HomeDetails",
					"target": [
						"businessCardList",
						"homeDetails"
					]
				},
				{
					"pattern": "BusinessCards/:id:",
					"name": "BusinessCards",
					"target": [
						"businessCardList",
						"businessCardDetails"
					]
				},
				{
					"pattern": "addBusinessCard",
					"name": "addBusinessCard",
					"target": [
						"businessCardList",
						"businessCardDetails"
					]
				}
			],
			"targets": {
				"homeDetails": {
					"viewName": "Home",
					"viewLevel": 2,
					"controlId": "splitApp",
					"controlAggregation": "detailPages",
					"clearTarget": true
				},
				"businessCardList": {
					"viewName": "BusinessCardList",
					"viewLevel": 1,
					"controlId": "splitApp",
					"controlAggregation": "masterPages",
					"clearTarget": true
				},
				"businessCardDetails": {
					"viewName": "BusinessCardDetails",
					"viewLevel": 2,
					"controlId": "splitApp",
					"controlAggregation": "detailPages"
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}